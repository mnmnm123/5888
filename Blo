local Library = {}
local LibraryName = tostring(math.random(100000,200000))..tostring(math.random(100000,200000))..tostring(math.random(100000,200000))

function Library:Toggle()
    if game.CoreGui:FindFirstChild(LibraryName).Enabled then 
        game.CoreGui:FindFirstChild(LibraryName).Enabled = false
    else 
        game.CoreGui:FindFirstChild(LibraryName).Enabled = true
    end
end

function Library:Drag(obj)
    local UserInputService = game:GetService("UserInputService")
    
    local gui = obj
    
    local dragging
    local dragInput
    local dragStart
    local startPos
    
    local function update(input)
        local delta = input.Position - dragStart
        gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
    
    gui.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
            dragging = true
            dragStart = input.Position
            startPos = gui.Position

            input.Changed:Connect(function()
                if input.UserInputState == Enum.UserInputState.End then
                    dragging = false
                end
            end)
        end
    end)
    
    gui.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragging then
            update(input)
        end
    end)
end

function Library:Create(xHubName,xGameName)
    local xHubName = xHubName or "UI Library"
    local xGameName = xGameName or "By Mapple#3045"
    local ScreenGui = Instance.new("ScreenGui")
    ScreenGui.DisplayOrder = 99999
    ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    ScreenGui.Parent = game.CoreGui
    ScreenGui.ResetOnSpawn = false 
    ScreenGui.Name = LibraryName

    local Main = Instance.new("Frame")
    Main.Name = "Main"
    Main.Parent = ScreenGui
    Main.BackgroundColor3 = Color3.fromRGB(31, 30, 46)
    Main.Position = UDim2.new(0.5, -232, 0.5, -148)
    Main.Size = UDim2.new(0, 464, 0, 296)
    Main.ClipsDescendants = false

    local MainCorner = Instance.new("UICorner")
    MainCorner.CornerRadius = UDim.new(0, 12)
    MainCorner.Name = "MainCorner"
    MainCorner.Parent = Main
    
    local GlowContainer = Instance.new("Frame")
    GlowContainer.Name = "GlowContainer"
    GlowContainer.Parent = Main
    GlowContainer.BackgroundTransparency = 1
    GlowContainer.Size = UDim2.new(1, 0, 1, 0)
    GlowContainer.ZIndex = -5
    
    local Particles = {}
    local particleCount = 120
    
    local function createParticleRing(radius, count, baseSize, speedMult, lifeMult)
        local ringParticles = {}
        for i = 1, count do
            local angle = (i / count) * math.pi * 2
            local particle = Instance.new("Frame")
            particle.Name = "ParticleRing_" .. radius .. "_" .. i
            particle.Parent = GlowContainer
            particle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            particle.BorderSizePixel = 0
            particle.Size = UDim2.new(0, baseSize, 0, baseSize)
            particle.AnchorPoint = Vector2.new(0.5, 0.5)
            particle.ZIndex = -3
            
            local particleCorner = Instance.new("UICorner")
            particleCorner.CornerRadius = UDim.new(1, 0)
            particleCorner.Parent = particle
            
            table.insert(ringParticles, {
                frame = particle,
                angle = angle,
                radius = radius,
                baseSize = baseSize,
                speed = 0.5 + math.random() * 0.8 * speedMult,
                life = 0,
                maxLife = 1 + math.random() * 2 * lifeMult,
                currentLife = 0,
                pulseOffset = math.random() * math.pi * 2
            })
        end
        return ringParticles
    end
    
    local ring1 = createParticleRing(15, 40, 3, 0.8, 0.8)
    local ring2 = createParticleRing(25, 50, 2, 1, 1)
    local ring3 = createParticleRing(35, 30, 1.5, 1.2, 1.2)
    
    for _, p in ipairs(ring1) do table.insert(Particles, p) end
    for _, p in ipairs(ring2) do table.insert(Particles, p) end
    for _, p in ipairs(ring3) do table.insert(Particles, p) end
    
    spawn(function()
        local time = 0
        while Main.Parent do
            time = time + 0.03
            
            for _, particleData in ipairs(Particles) do
                particleData.currentLife = particleData.currentLife + particleData.speed * 0.03
                
                if particleData.currentLife > particleData.maxLife then
                    particleData.currentLife = 0
                    particleData.life = 0
                end
                
                local lifeProgress = particleData.currentLife / particleData.maxLife
                local pulse = math.sin(time * 2 + particleData.pulseOffset) * 0.3 + 0.7
                
                local distance = particleData.radius + lifeProgress * 40
                local xPos = 0.5 + math.cos(particleData.angle) * (distance / 232)
                local yPos = 0.5 + math.sin(particleData.angle) * (distance / 148)
                
                particleData.frame.Position = UDim2.new(xPos, 0, yPos, 0)
                
                local sizeMult = (1 - lifeProgress) * pulse
                local finalSize = math.max(0.5, particleData.baseSize * sizeMult)
                particleData.frame.Size = UDim2.new(0, finalSize, 0, finalSize)
                
                local transparency = 0.1 + lifeProgress * 0.9
                particleData.frame.BackgroundTransparency = transparency
                
                local intensity = 1 - lifeProgress * 0.7
                local colorValue = math.floor(200 + 55 * intensity)
                particleData.frame.BackgroundColor3 = Color3.fromRGB(colorValue, colorValue, colorValue)
            end
            
            wait(0.03)
        end
    end)

    local CloseButton = Instance.new("TextButton")
    local CloseButtonCorner = Instance.new("UICorner")

    CloseButton.Name = "CloseButton"
    CloseButton.Parent = Main
    CloseButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    CloseButton.Position = UDim2.new(0, 5, 0, 5)
    CloseButton.Size = UDim2.new(0, 25, 0, 25)
