    local function startRainbowEffect()
        coroutine.wrap(function()
            while HubName.Parent ~= nil do
                for i = 1, #rainbowColors do
                    if HubName.Parent == nil then break end
                    TweenService:Create(HubName, TweenInfo.new(0.5), {
                        TextColor3 = rainbowColors[i]
                    }):Play()
                    wait(0.5)
                end
            end
        end)()
    end

    function ScrollSize()
        ActualSide.CanvasSize = UDim2.new(0, 0, 0, ActualSideListLayout.AbsoluteContentSize.Y + 100)
    end

    Sidebar.Name = "Sidebar"
